{
  "name": "@jym272ticketing/common",
  "version": "1.0.79",
  "description": "https://dev.to/franciscomendes10866/how-to-setup-a-node-api-with-swc-and-eslint-1h5d | https://amanhimself.dev/blog/setup-nextjs-project-with-eslint-prettier-husky-lint-staged/",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/server.js",
    "build": "bash scripts/build",
    "pub": "npm run build && npm version patch && npm publish --access public",
    "start": "NODE_ENV=production node dist/server.js",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts,.js src tests",
    "lint:fix": "eslint --ext .ts src --fix",
    "prepare": "[ \"$NODE_ENV\" = 'production' ] || husky install .husky",
    "lint-staged": "lint-staged",
    "test": "bash scripts/run-test-local",
    "show-report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.29.2",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "@types/cookie-session": "^2.0.44",
    "cookie-session": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "nats": "^2.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.3",
    "typescript": "^4.9.4"
  }
}
